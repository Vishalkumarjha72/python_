     "text": [
      "12 3 34 453 65 66 3 \n",
      "-412\n"
     ]
    }
   ],
   "source": [
    "l =[int(s) for s in input().split()]\n",
    "even = 0\n",
    "odd = 0\n",
    "for i in l:\n",
    "    if i %2 ==0:\n",
    "        even += i\n",
    "    else:\n",
    "        odd +=i\n",
    "print(even - odd)"
